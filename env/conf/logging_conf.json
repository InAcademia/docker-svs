{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "message_only": {
            "format": "%(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "message_only",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "svs": {
            "level": "DEBUG",
            "handlers": ["console"],
            "propagate": false
        }
    },
    "root": {
        "level": "INFO",
        "handlers": ["console"]
    }
}